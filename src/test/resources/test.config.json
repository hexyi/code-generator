{
  "types": [
    {
      "name": "model",
      "template": "Entity.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.domain",
      "suffix": ".java",
      "dependencies": []
    },
    {
      "name": "dao",
      "template": "Dao.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.dao",
      "suffix": "Dao.java",
      "dependencies": []
    },
    {
      "name": "mapper",
      "template": "Mapper.xml.ftl",
      "target": "D:/",
      "pkg": "mybatis.mappers",
      "suffix": "Mapper.xml",
      "dependencies": []
    }
  ],
  "tables": [
    {
      "name": "sys_user",
      "className": "User",
      "pk": "id",
      "updates": [
        "username",
        "email"
      ],
      "queries":{
        "username":"=",
        "email:":"="
      },
      "ignoreTypes": [
        "model",
        "dao"
      ],
      "types": [
        {
          "name": "dto",
          "impls": [
            {
              "name": "UserRequest",
              "columns": [
                "username",
                "email"
              ]
            }
          ]
        }
      ]
    }
  ]
}