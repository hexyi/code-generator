{
  "author": "fenghao",
  "version": "1.0",
  "since": "1.0",
  "templateDir": "templates",
  "jdbcDriver": "com.mysql.jdbc.Driver",
  "jdbcUrl": "jdbc:mysql://localhost:3306/test?useUnicode=true&characterEncoding=UTF-8",
  "jdbcUsername": "root",
  "jdbcPassword": "",
  "database": "mysql",
  "ignoreTables": [],
  "types": [
    {
      "name": "model",
      "template": "Entity.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.domain",
      "suffix": "${ClassName}.java",
      "dependencies": []
    },
    {
      "name": "dao",
      "template": "Dao.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.dao",
      "suffix": "${ClassName}Dao.java",
      "dependencies": []
    },
    {
      "name": "mapper",
      "template": "Mapper.xml.ftl",
      "target": "D:/",
      "pkg": "mybatis.mappers",
      "suffix": "${ClassName}Mapper.xml",
      "dependencies": []
    },
    {
      "name": "dto",
      "template": "DTO.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.dto",
      "suffix": "${implClassName}.java",
      "dependencies": []
    },
    {
      "name": "convert",
      "template": "Cvt.java.ftl",
      "target": "D:/",
      "pkg": "io.github.howiefh.cvt",
      "suffix": "${ClassName}Convertor.java",
      "dependencies": [
        "model",
        "dto"
      ]
    }
  ],
  "tables": [
    {
      "name": "sys_user",
      "className": "User",
      "pks": ["id"],
      "updates": [
        "username",
        "email"
      ],
      "queries":{
        "username":"=",
        "email:":"="
      },
      "ignoreTypes": [
      ],
      "types": [
        {
          "name": "dto",
          "impls": [
            {
              "name": "UserRequest",
              "columns": [
                "username",
                "password",
                "email"
              ]
            }
          ]
        }
      ]
    }
  ]
}